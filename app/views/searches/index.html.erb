<div class="container py-5">
  <h1>Search#index</h1>
  <p>Find me in app/views/searches/index.html.erb</p>
  <hr>
  <ul class="list-unstyled">
    <% @itineraries.each do |itinerary| %>
      <li id="itinerary-id-<%= itinerary.id %>">
        <%= link_to raw('<i class="far fa-trash-alt"></i>'), "#{itinerary_path(itinerary)}", method: :delete, class: 'text-decoration-none text-secondary' %>
        &emsp;
        <%= link_to "#{itinerary.country}, #{itinerary.experience} experience", itinerary_path(itinerary) %>
        </li>
    <% end %>
  </ul>
</div>
